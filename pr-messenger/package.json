{
    "name": "pr-messenger",
    "version": "0.0.10",
    "description": "",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "postinstall": "pnpm dlx rimraf node_modules/.cache",
        "dev": "webpack --config webpack.dev.js",
        "build": "webpack --config webpack.build.js",
        "lint": "pnpm run \"/^lint:.*/\"",
        "lint:eslint": "eslint --ext .js,.ts . --max-warnings=-1 --cache --cache-location node_modules/.cache/eslint",
        "lint:types": "pnpm exec tsc",
        "clean": "pnpm dlx rimraf dist node_modules/.cache",
        "reset": "pnpm run clean && pnpm run reset:modules",
        "reset:modules": "pnpm dlx rimraf node_modules pnpm-lock.yaml",
        "analyze": "webpack --config webpack.build.js --analyze"
    },
    "keywords": [],
    "author": "alexandre.beaudoin@gsoft.com",
    "license": "Apache-2.0",
    "dependencies": {
        "azure-devops-node-api": "12.0.0",
        "azure-pipelines-task-lib": "4.3.1"
    },
    "devDependencies": {
        "@swc/core": "1.3.62",
        "@types/node": "18.16.16",
        "@types/q": "1.5.5",
        "@workleap/eslint-plugin": "1.8.1",
        "copy-webpack-plugin": "11.0.0",
        "eslint": "8.42.0",
        "swc-loader": "0.2.3",
        "terser-webpack-plugin": "5.3.9",
        "typescript": "5.1.3",
        "webpack": "5.86.0",
        "webpack-bundle-analyzer": "4.9.0",
        "webpack-cli": "5.1.4"
    }
}
